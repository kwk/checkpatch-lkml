+	int __done = 0;						\
+	int __t = timeout;					\
+	while (__t) {						\
+		__done = lcd_readl(bfi, reg) & (intr);		\
+		if (__done)					\
+			break;					\
+		mdelay(10);					\
+		__t--;						\
+	}							\
+	if (!__t)						\
+		dev_info(bfi->dev, "wait " #intr " timeount");	\
+	__done;							\
+})

