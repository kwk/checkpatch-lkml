+{						\
+	.task		= &tsk,			\
+	.exec_domain	= &default_exec_domain,	\
+	.cpu		= 0,			\
+	.preempt_count	= 1,			\
+	.addr_limit	= KERNEL_DS,		\
+	.restart_block	= {			\
+		.fn = do_no_restart_syscall,	\
+	},					\
+}

WARNING:SPACE_BEFORE_TAB: please, no space before tabs
#239: FILE: arch/score/include/asm/thread_info.h:66:
