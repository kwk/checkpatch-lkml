+									\
+	e_type								\
+	name##_hash_lookup(bfa_ht_t ht_hdl, e_type he_with_key)		\
+	{								\
+		struct bfa_q_s *q, *qe;					\
+		int h_val;						\
+									\
+		/* Basic checking */					\
+		bfa_assert(ht_hdl != NULL && he_with_key != NULL);	\
+									\
+		/* Hash the data */					\
+		h_val = h_fn(he_with_key);				\
+		bfa_assert(h_val >= 0);					\
