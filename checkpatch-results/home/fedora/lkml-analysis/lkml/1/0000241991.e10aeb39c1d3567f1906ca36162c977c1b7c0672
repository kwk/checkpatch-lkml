#154: FILE: kernel/rcuclassic.c:328:
+	} else if (!cpus_empty(rcp->cpumask) && delta >= 2L) {
+

CHECK:BRACES: Blank lines aren't necessary before a close brace '}'
#159: FILE: kernel/rcuclassic.c:333:
+
+	}

