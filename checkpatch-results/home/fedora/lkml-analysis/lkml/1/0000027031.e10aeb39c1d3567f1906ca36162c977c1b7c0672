+#define __STRUCT_KFIFO(type, size, recsize, ptrtype) \
+{ \
+	__STRUCT_KFIFO_COMMON(recsize, ptrtype) \
+	type		data[(!size || (size & (size - 1))) ? -1 : size]; \
+}

