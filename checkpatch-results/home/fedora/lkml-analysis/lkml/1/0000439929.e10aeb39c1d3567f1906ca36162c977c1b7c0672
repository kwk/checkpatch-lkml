#100: FILE: drivers/s390/net/ctcmain.c:2745:
+	priv = kzalloc(sizeof(struct ctc_priv), GFP_KERNEL);

CHECK:ALLOC_SIZEOF_STRUCT: Prefer kzalloc(sizeof(*dev)...) over kzalloc(sizeof(struct net_device)...)
#116: FILE: drivers/s390/net/ctcmain.c:2792:
+		dev = kzalloc(sizeof(struct net_device), GFP_KERNEL);

