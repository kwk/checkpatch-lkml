+			ss->attach(ss, cont, oldcont, tsk);
+		}

WARNING:SSCANF_TO_KSTRTO: Prefer kstrto<type> to single variable sscanf
#150: FILE: kernel/container.c:757:
+	if (sscanf(pidbuf, "%d", &pid) != 1)
