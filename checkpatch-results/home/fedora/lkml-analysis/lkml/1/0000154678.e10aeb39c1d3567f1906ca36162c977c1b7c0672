+		if (__pport->bfa) {      \
+			(__pport)->event_cbfn((__pport)->event_cbarg, (__event) );      \
+		} else {							\
+			(__pport)->hcb_event = (__event );      \
+			bfa_cb_queue((__pport)->bfa, &(__pport)->hcb_qe,	\
+					__bfa_cb_port_event, (__pport) );      \
+		}								\
+	}\
+} while (0)

