#681: FILE: arch/ppc/power/swsusp2.c:40:
+volatile static unsigned long * origpage __nosavedata;

ERROR:POINTER_LOCATION: "foo * bar" should be "foo *bar"
#682: FILE: arch/ppc/power/swsusp2.c:41:
