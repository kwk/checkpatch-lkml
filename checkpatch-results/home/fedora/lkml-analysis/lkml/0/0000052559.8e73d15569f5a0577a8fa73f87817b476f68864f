#118: FILE: arch/i386/boot/compressed/misc.c:139:
+static unsigned serial_base = DEFAULT_BASE;

CHECK:SPACING: No space is necessary after a cast
#172: FILE: arch/i386/boot/compressed/misc.c:230:
+		vidmem = (char *) 0xb0000;

CHECK:SPACING: No space is necessary after a cast
#175: FILE: arch/i386/boot/compressed/misc.c:233:
