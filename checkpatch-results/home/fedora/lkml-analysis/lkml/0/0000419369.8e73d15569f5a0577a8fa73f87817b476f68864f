+		case KEY_MINUS:
+		case KEY_EQUAL:
+		case KEY_GRAVE:
+		case KEY_SEMICOLON:
+		case KEY_APOSTROPHE:
[...]
+		case KBD98_KEY_NULL:
[...]
+		case 0:
[...]
+		default:

ERROR:SPACING: space prohibited before that '++' (ctx:WxO)
#236: FILE: drivers/input/keyboard/98kbd.c:150:
