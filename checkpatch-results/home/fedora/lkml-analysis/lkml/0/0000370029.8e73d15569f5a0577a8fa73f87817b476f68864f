+	switch (uhci->state) {
+		case UHCI_SUSPENDED:		/* Start the resume */
[...]
+		case UHCI_RESUMING_1:		/* Continue the resume */
[...]
+		case UHCI_RESUMING_2:		/* Run for at least 1 second */
[...]
+		case UHCI_RUNNING_GRACE:	/* Now allowed to suspend */
[...]
+		default:

