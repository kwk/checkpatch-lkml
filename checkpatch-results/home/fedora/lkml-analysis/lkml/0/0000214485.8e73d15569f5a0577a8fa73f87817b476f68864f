#326: FILE: arch/m32r/kernel/sys_m32r.c:174:
+		if (!access_ok(VERIFY_WRITE, (ulong __user *) third,

CHECK:PARENTHESIS_ALIGNMENT: Alignment should match open parenthesis
#327: FILE: arch/m32r/kernel/sys_m32r.c:175:
