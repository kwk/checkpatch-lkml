#102: FILE: drivers/usb/input/appletouch.c:88:
+struct rchan_callbacks* rcb = NULL;

ERROR:POINTER_LOCATION: "foo* bar" should be "foo *bar"
#102: FILE: drivers/usb/input/appletouch.c:88:
