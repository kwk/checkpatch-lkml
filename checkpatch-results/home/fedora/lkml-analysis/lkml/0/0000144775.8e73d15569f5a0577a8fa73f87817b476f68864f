+        newval = pid | waitflag | deadflag;                            \
+      } while (atomic_compare_and_exchange_bool_acq (__futex, newval, oldval) != 0); \
+      __val = 0;                                                       \
+      if (waitflag != 0)                                               \
+        __val = lll_futex_wait_robust(futex);                          \
+    } while (__val == -EAGAIN);                                        \
+    __val;                                                             \
+  })

