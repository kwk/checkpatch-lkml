+#define __lll_mutex_lock_robust(futex)                                 \
+  ({                                                                   \
+    int *__futex = (futex);                                            \
+    int __val;                                                         \
+    int oldval, newval;                                                \
+    int pid, deadflag, waitflag;                                       \
+                                                                       \
+    do {                                                               \
+      do {                                                             \
+        oldval = *__futex;                                             \
+        pid = oldval & 0x3fff;                                         \
+        deadflag = oldval & FUTEX_OWNER_DIED;                          \
+        if (pid == 0) {                                                \
+          waitflag = 0;                                                \
+          pid = THREAD_GETMEM (THREAD_SELF, tid);                      \
+        }                                                              \
