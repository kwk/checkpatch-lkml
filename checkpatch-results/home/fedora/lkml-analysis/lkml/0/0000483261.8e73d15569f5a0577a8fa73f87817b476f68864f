#356: FILE: drivers/ide/ide-pci.c:875:
+			if ((findev->vendor == dev->vendor) &&
+			    (findev->device == dev->device) &&
+			    (PCI_SLOT(findev->devfn) & 2)) {

CHECK:UNNECESSARY_PARENTHESES: Unnecessary parentheses around 'findev->device == dev->device'
