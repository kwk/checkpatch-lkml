+	dir = accessfs_make_dirpath("capabilities");

WARNING:ARRAY_SIZE: Prefer ARRAY_SIZE(names)
#268: FILE: fs/accessfs/capabilities.c:99:
+	unregister_capabilities(dir, sizeof(names) / sizeof(names[0]));

