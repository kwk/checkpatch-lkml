#313: FILE: mm/oom_kill.c:313:
+	printk(KERN_CRIT "Killing process %d (%s) - %s\n",

CHECK:PARENTHESIS_ALIGNMENT: Alignment should match open parenthesis
#314: FILE: mm/oom_kill.c:314:
+	printk(KERN_CRIT "Killing process %d (%s) - %s\n",
+					current->pid, current->comm, msg);

